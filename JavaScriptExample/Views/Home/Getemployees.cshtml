@model List<JavaScriptExample.Models.Employee>

@{
    ViewBag.Title = "Getemployees";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Getemployees</h2>


<table class="table table-bordered" id="EmployeesTable">
    <thead>
        <th>ID</th>
        <th>Name</th>
        <th>Department</th>
        <th>Salary</th>
         <th>Age</th>
    </thead>
    <tbody>
        @foreach (var employee in Model) { 
        <tr>
            <td>@employee.ID</td>
            <td>@employee.Name</td>
            <td>@employee.Department</td>
            <td>@employee.Salary</td>
            <td>@employee.Age</td>
        </tr>
        }
    </tbody>

</table>


